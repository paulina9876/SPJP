#include<stdio.h>

struct Dane
{
    char *imie;
    char *nazwisko;
    int nr_telefonu;
}bartek;

int main()
{
    struct Dane kamil;
    
    kamil.imie="Kamil";
    kamil.nazwisko="Nowak";
    kamil.nr_telefonu=123456789;
    
    bartek.imie="Bartek";
    bartek.nazwisko="Kowalski";
    bartek.nr_telefonu=987654321;
    
    printf(" imie: %s\n nazwisko: %s\n nr telefonu: %d\n ",kamil.imie,kamil.nazwisko,kamil.nr_telefonu);
    printf("\n imie: %s\n nazwisko: %s\n nr telefonu: %d\n ",bartek.imie,bartek.nazwisko,bartek.nr_telefonu);
    
    return 0;
    
}

