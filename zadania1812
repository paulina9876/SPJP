zadanie5.c
#include <stdio.h>

int main ()

{

    int tab[] = (a, b, c);
    a = 7;
    b = 2;
    c = 8;
    
    for(int i=0;i<3;i++);
    sort(tab, tab+3);
    printf<<tab[i]<<' ';
    return 0;
    

}

zadanie6.c
#include <stdio.h>

int main ()

{
    int tab [n];
    tab[0] = 1;
    tab[1] = 2;
    tab[2] = 3;
    tab[n-1] = n;
    tab[n] = n+1;
    for (int n>1; tab[n]=n+1; tab[n-1]=n);
    printf("Podaj n\n");
}
